{
	"description": "Maps data from MQ to the database",
	"map": {
		"identifier": {
			"source": {
				"$ref": "schemas/database/airlines.schema.json#/properties/id"
			},
			"destination": {
				"$ref": "schemas/database/pilots.schema.json#/properties/airline_id"
			},
			"multiplicity": "*-1"
		},
		"name": {
			"source": {
				"$ref": "schemas/database/airlines.schema.json#/properties/name"
			},
			"destination": {
				"$ref": "schemas/database/pilots.schema.json#/properties/name"
			},
			"multiplicity": "*-1"
		},
		"country": {
			"source": {
				"$ref": "schemas/database/countries.schema.json#/properties/code"
			},
			"destination": {
				"$ref": "schemas/database/pilots.schema.json#/properties/country"
			},
			"multiplicity": "*-1"
		}
	}
}