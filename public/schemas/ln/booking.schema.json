{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://yourdomain.com/schemas/mq/booking.schema.json",
  "title": "booking",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "Identifier for the booking"
    },
    "airline": {
      "type": "number",
      "description": "Identifier for the airline"
    },
    "name": {
      "type": "string",
      "description": "The name of the passenger"
    },
    "date": {
      "type": "date-time",
      "description": "The date of the booking"
    }
  }
}